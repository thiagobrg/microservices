server:
  ssl:
    enabled: true
    key-store: file:/opt/secret/keystore.p12
    key-store-password: ${SMARTRTT_KEY_STORE_PASSWORD:default}
    key-store-type: PKCS12
    trust-store: file:/opt/secret/truststore.p12
    trust-store-password: ${SMARTRTT_KEY_STORE_PASSWORD:default}
    trust-store-type: PKCS12